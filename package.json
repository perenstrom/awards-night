{
  "name": "awards-night",
  "version": "4.4.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack -p 3400",
    "build": "next build",
    "start": "next start",
    "what": "ECHO Package system: npm",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "next lint",
    "tsc": "tsc --noemit",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:migrate:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "prisma:migrate:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "prisma:seed:local": "dotenv -e .env.local -- npx prisma db seed",
    "prisma:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:push:local": "dotenv -e .env.local -- npx prisma db push"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.0.0-beta.10",
    "clsx": "^2.1.0",
    "next": "^15.1.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sass": "^1.70.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.23.1",
    "eslint-config-next": "^15.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.49",
    "prisma": "^6.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "./prisma/seed.sh"
  }
}
